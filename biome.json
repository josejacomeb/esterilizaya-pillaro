			"onpointermove",
			"ontoggle",
			"oncontextmenu",
			"onblur",
			"oncancel",
			"onbeforeprint",
			"oncontextrestored",
			"onloadedmetadata",
			"onpointerup",
			"onlanguagechange",
			"jQuery",
			"onselectstart",
			"oncopy",
			"onscroll",
			"onload",
			"ondragstart",
			"onbeforeinput",
			"oncanplaythrough",
			"oninput",
			"oninvalid",
			"ontimeupdate",
			"ondurationchange",
			"onselectionchange",
			"onmouseup",
			"location",
