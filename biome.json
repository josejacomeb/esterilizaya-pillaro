			"onpointerleave",
			"oncontextlost",
			"ondrop",
			"onsecuritypolicyviolation",
			"oncontentvisibilityautostatechange",
			"ondeviceorientation",
			"onseeking",
			"onrejectionhandled",
			"onunload",
			"onmouseleave",
			"onhashchange",
			"onpointerenter",
