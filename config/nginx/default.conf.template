# upstream for uWSGI
upstream uwsgi_app {
    server unix:/home/esterilizaya/code/uwsgi_app.sock;
}
server {
    listen      80;
    server_name www.happypawspillaro.com happypawspillaro.com;
    error_log   stderr warn;
    access_log /dev/stdout main;
    location / {
        include /etc/nginx/uwsgi_params;
        uwsgi_pass uwsgi_app;
    }
}